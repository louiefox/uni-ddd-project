@page "/login"


<form name ="login_form">

    <div class="login_form_email">
        <label>Email</label><br/>
        <input type ="text" name="email">       
    </div>

    <div class ="login_form_password">
        <label>Password</label><br/>
        <input type="password" name="password">
    </div>

    <div class = "login_form_buttom">
        <a href = "" class = "btn_signin">Sign in</a>
        <h1>or</h1>
        <a href = "" class = "btn_signup">Sign up</a>
    </div>

    <div class="login_image">
        <img src="https://img.freepik.com/free-vector/group-college-university-students-hanging-out_74855-5251.jpg?w=2000&t=st=1679586756~exp=1679587356~hmac=9a27a40cbca2fa3817d01e5614e0da3a55e7287bc9806822b2e7365372de1b7b" width="690" height="500">
    </div>
    

    

</form>


